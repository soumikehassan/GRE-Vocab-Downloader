{
  "manifest_version": 3,
  "name": "GRE Vocab & Downloader",
  "version": "1.3",
  "permissions": [
    "contextMenus",
    "tts",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://api.dictionaryapi.dev/*"
  ]
}